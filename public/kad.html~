<html>
  <head>
    <script   src="https://code.jquery.com/jquery-3.1.0.min.js"   integrity="sha256-cCueBR6CsyA4/9szpPfrX3s49M9vUU5BgtiJj06wt/s="   crossorigin="anonymous"></script>
    <style>
      div#header {
      height: 4%;
      }
      
      iframe#frame {
      width: 100%;
      height: 94%;
      }
    </style>
  </head>
  <body>
    <div id="header">
      address:
      <input type="text" id="addressBar"/>
      <input type="button" value="thing" id="button"/>
    </div>
    <iframe id="frame"></iframe>
    <script>
      var addressBar = $('div#header input#addressBar')
      var frame = $('iframe#frame')
      var button = $('div#header input#button')
      var currentLocation;

      frame.get(0).onload = function() {
      console.log(this)
//      debugger
      currentLocation = this.contentWindow.document.location
      console.log(currentLocation)
      addressBar.val(currentLocation)
      }

      addressBar.keypress(function(event) { 
      if (event.keyCode === 13) {
      currentLocation = addressBar.val()
      frame.attr('src', currentLocation)
      }
      })

      button.click(function() {
      console.log(frame)
      })
    </script>
  </body>
